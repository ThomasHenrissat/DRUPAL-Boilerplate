{#
/**
* @file
* Theme override to display a media item.
*
* Available variables:
* - name: Name of the media.
* - content: Media content.
*
* @see template_preprocess_media()
*/
#}

{% if content %}
    {% include "@templates/objects/media/_media.html.twig" with {
        image: content.field_media_image
    } %}
{% endif %}